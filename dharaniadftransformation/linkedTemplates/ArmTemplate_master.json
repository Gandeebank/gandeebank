{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dharaniadftransformation"
		},
		"ADFServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ADFServer'",
			"defaultValue": "Integrated Security=False;Data Source=gandeebanshelllab.database.windows.net;Initial Catalog=GandeebanlabDB;User ID=gandeebank_adf"
		},
		"AzureBlobDSTStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobDSTStorage1'"
		},
		"SQLServerName_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQLServerName'",
			"defaultValue": "Integrated Security=False;Data Source=GandeebanSQLVM;Initial Catalog=EmployeeDB;User ID=gandeebank_adf"
		},
		"ls_adls_source_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_source'"
		},
		"ls_adls_target_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_target'"
		},
		"AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://GKkeyvault.vault.azure.net/"
		},
		"ls_adls_source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dharanistorage.dfs.core.windows.net/"
		},
		"ls_adls_target_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dharanistorage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dharaniadftransformation_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobDSTStorage1_connectionString": {
						"value": "[parameters('AzureBlobDSTStorage1_connectionString')]"
					},
					"ls_adls_source_accountKey": {
						"value": "[parameters('ls_adls_source_accountKey')]"
					},
					"ls_adls_target_accountKey": {
						"value": "[parameters('ls_adls_target_accountKey')]"
					},
					"ADFServer_connectionString": {
						"value": "[parameters('ADFServer_connectionString')]"
					},
					"SQLServerName_connectionString": {
						"value": "[parameters('SQLServerName_connectionString')]"
					},
					"AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"ls_adls_source_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_source_properties_typeProperties_url')]"
					},
					"ls_adls_target_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_target_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}